                itemWidth: 25,      // åœ†å½¢å°ºå¯¸
                itemHeight: 25,
                data: getLegendData(), // åˆå§‹åŒ–å›¾ä¾‹é¢œè‰²
                // å¼ºåˆ¶æ‰€æœ‰ç³»åˆ—é»˜è®¤ä¸ºâ€œé€‰ä¸­â€ï¼Œé˜²æ­¢EChartsè‡ªåŠ¨å˜ç°
                selected: {
                    'çº¢è‰²ç³»åˆ—': true,
