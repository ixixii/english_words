  <script>
    const chart = echarts.init(document.getElementById('chart'));

    const colorMap = {
      çº¢è‰²: [255, 0, 0],
      ç»¿è‰²: [0, 255, 0],
      è“è‰²: [0, 0, 255]
    };

    function rgba(name, alpha) {
      const [r, g, b] = colorMap[name];
      return `rgba(${r},${g},${b},${alpha})`;
