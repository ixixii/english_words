        var myChart = echarts.init(document.getElementById('main'));

        // === 1. å®šä¹‰åŸå§‹æ•°æ®å’Œé¢œè‰²é…ç½® ===
        // æˆ‘ä»¬å¿…é¡»æŠŠæ•°æ®å­˜ä¸‹æ¥ï¼Œå› ä¸ºç‚¹å‡»éšè—æ—¶ï¼Œæˆ‘ä»¬ä¼šæŠŠå›¾è¡¨é‡Œçš„æ•°æ®åˆ æ‰
        var rawData = {
