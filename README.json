          stack: 'total',
          data: [10, 20, 30]
        },
        {
          name: 'ç»¿è‰²',
          type: 'bar',
          stack: 'total',
          data: [15, 25, 35]
        },
        {
          name: 'è“è‰²',
          type: 'bar',
          stack: 'total',
          data: [5, 10, 15]
        }
      ]
    };

    function updateStyle() {
      const selected = option.legend.selected || {};

      // legend æ–‡å­—
      Object.keys(colorMap).forEach(name => {
        const active = selected[name] !== false;
        option.legend.textStyle.rich[name] = {
