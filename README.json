            align-items: center;
            margin: 6px 0;
            cursor: pointer;
            font-size: 12px;
        }
        .legend-icon {
            width: 12px;
            height: 12px;
            margin-right: 6px;
            border-radius: 2px;
            transition: opacity 0.3s;
        }
        .legend-text {
            color: #333;
            transition: color 0.3s;
        }
        .legend-item.disabled .legend-icon {
            opacity: 0.4;
        }
