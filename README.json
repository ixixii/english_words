        }

        var option = {
            title: {
                text: 'ç‚¹å‡»å›¾ä¾‹ -> æŸ±å­æ¶ˆå¤± + å›¾ä¾‹å˜æ·¡(ä¸ç°è‰²)',
                left: 'center',
