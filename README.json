<script setup>
import { ref, onMounted, onUnmounted, computed, watch } from 'vue';
import * as echarts from 'echarts';

// --- 1. 基础响应式逻辑 ---
const DESIGN_WIDTH = 1920; // 假设开发基准是 1920
const windowWidth = ref(window.innerWidth);
const scale = computed(() => windowWidth.value / DESIGN_WIDTH);

// 防抖处理 resize
let timer = null;
const handleResize = () => {
  if (timer) clearTimeout(timer);
  timer = setTimeout(() => {
    windowWidth.value = window.innerWidth;
    if (myChart) {
      myChart.resize();
      renderChart(); // 窗口变化时重新计算内部 px 比例
    }
  }, 150);
};

// --- 2. ECharts 初始化与配置 ---
const chartRef = ref(null);
let myChart = null;

const renderChart = () => {
  if (!myChart) return;

  const s = scale.value; // 当前缩放比例

  const option = {
    backgroundColor: '#0f1215',
    series: [
      // --- 图层一：警戒线层 ---
      {
        type: 'gauge',
        radius: '82%', 
        startAngle: 225,
        endAngle: -45,
        min: 0,
        max: 100,
        axisLine: {
          lineStyle: {
            width: 25 * s, // 适配线段纵向长度
            color: [
              [0.295, 'transparent'],
              [0.305, '#ff4d4f'],    
              [1, 'transparent']
            ]
          }
        },
        splitLine: { show: false },
        axisTick: { show: false },
        axisLabel: { show: false },
        pointer: { show: false },
        detail: { show: false }
      },

      // --- 图层二：主仪表盘与悬浮指针 ---
      {
        type: 'gauge',
        radius: '80%',
        startAngle: 225,
        endAngle: -45,
        min: 0,
        max: 100,
        axisLine: {
          lineStyle: {
            width: 12 * s, // 适配主圆盘厚度
            color: [[0.3, '#41b1ff'], [0.7, '#1890ff'], [1, '#0050b3']]
          }
        },
        pointer: {
          icon: 'path://M0 0 L100 0 L50 100 Z',
          length: '15%',
          width: 18 * s, // 适配三角形宽度
          offsetCenter: [0, '-120%'], 
          itemStyle: {
            color: '#ffcc00',
            shadowBlur: 8 * s, // 适配阴影模糊度
            shadowColor: 'rgba(255,204,0,0.6)'
          }
        },
        anchor: { show: false },
        title: { show: false },
        // 刻度样式适配
        axisTick: { 
