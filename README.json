      // series & icon
      option.series.forEach(s => {
        const active = selected[s.name] !== false;
        s.itemStyle = {
          color: rgba(s.name, active ? 1 : 0.5)
        };
      });
    }

    updateStyle();
    chart.setOption(option);

    chart.on('legendselectchanged', function () {
