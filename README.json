            color: #999;
        }
    </style>
</head>
<body>
    <div class="chart-box">
        <div id="chart"></div>
        <div class="legend-box" id="legend"></div>
    </div>

    <script>
        // 1. åˆå§‹åŒ–
        const chart = echarts.init(document.getElementById('chart'));
        
        // 2. æ•°æ®
        const seriesList = [
            { name: 'äº§å“A', color: '#ff0000', data: [320, 332, 301, 334, 390, 330, 320] },
            { name: 'äº§å“B', color: '#0000ff', data: [120, 132, 101, 134, 90, 230, 210] },
            { name: 'äº§å“C', color: '#00ff00', data: [220, 182, 191, 234, 290, 330, 310] }
        ];
        
        // 3. çŠ¶æ€
        const isVisible = {};
        seriesList.forEach(s => isVisible[s.name] = true);
        
        // 4. åˆ›å»ºå›¾è¡¨
        function initChart() {
            chart.setOption({
                tooltip: { trigger: 'axis' },
                legend: { show: false },
                xAxis: { type: 'category', data: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun'] },
                yAxis: { type: 'value' },
                series: seriesList.map(s => ({
                    name: s.name,
                    type: 'bar',
                    data: isVisible[s.name] ? s.data : s.data.map(() => null),
                    itemStyle: { color: s.color }
                }))
            });
        }
        
        // 5. åˆ›å»ºå›¾ä¾‹
        function createLegend() {
            const legend = document.getElementById('legend');
            legend.innerHTML = '';
            
            seriesList.forEach(s => {
                const div = document.createElement('div');
                div.className = `legend-item ${isVisible[s.name] ? '' : 'disabled'}`;
                div.innerHTML = `
                    <div class="legend-icon" style="background:${s.color};"></div>
                    <div class="legend-text">${s.name}</div>
                `;
                
                div.onclick = () => {
                    isVisible[s.name] = !isVisible[s.name];
                    div.classList.toggle('disabled');
                    
                    // æ›´æ–°å›¾è¡¨
                    const option = chart.getOption();
                    const index = seriesList.findIndex(item => item.name === s.name);
                    option.series[index].data = isVisible[s.name] ? s.data : s.data.map(() => null);
                    chart.setOption(option);
                };
                
                legend.appendChild(div);
            });
        }
        
        // 6. åˆå§‹åŒ–
        initChart();
        createLegend();
        
        // 7. å“åº”å¼
        window.onresize = () => chart.resize();
    </script>
</body>
</html>
